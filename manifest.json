{
    "manifest_version" : 2,
    "name" : "TimeZone / Appfinder",
    "version" : "1.0.0",
    "description" : "Shortcuts to social accounts",
    "icons" : {
        "128" : "clock.png"
    },
    "browser_action"   : {
        "default_title" : "Timezone/Appfinder",
        "default_popup" : "index.html",
        "default_icon" : "clock.png"
    },
    "permissions" : ["activeTab"],
    "background" : {
        "scripts" : ["app.js"]
    }




}